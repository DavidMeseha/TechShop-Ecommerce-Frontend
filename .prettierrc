{
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-organize-attributes"],
  "printWidth": 120,
  "singleQuote": false,
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "crlf",
  "attributeGroups": ["^class$", "^(id|name)$", "$DEFAULT", "^aria-"],
  "attributeSort": "ASC"
}
